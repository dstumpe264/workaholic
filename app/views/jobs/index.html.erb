<h1>Shows all jobs a user has</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Wage</th>
    <th></th>
  </tr>
    <% @user.jobs.each do |job| %>
      <tr>
        <td><%= job.title %></td>
        <td><%= link_to 'Show', job_path(job) %></td>
        <td><%= link_to 'Edit', edit_job_path(job) %></td>
        <td><%= link_to 'Destroy', job_path(job),
          method: :delete,
          data: {confirm: 'Are you sure?'} %></td>
      </tr>
    <% end %>
</table>
